			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".ExemploMapa" android:label="MapaCristo" />
		<activity android:name=".ExemploMapaGPS" android:label="MapaGPS" />
	</application>
	<uses-sdk android:minSdkVersion="2" />
</manifest> 